    "test": "react-vessel test"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=6"
  },
  "dependencies": {
    "@mornya/react-helper": "latest",
    "@mornya/react-social-libs": "latest",
    "classnames": "latest",
    "immer": "latest",
    "normalize.css": "latest",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.9",
    "@types/jest-diff": "latest",
    "@types/node": "^11.9.5",
    "@types/react": "^16.8.5",
    "@types/react-dom": "^16.8.2",
    "@types/react-redux": "^7.0.1",
    "@types/react-router-dom": "^4.3.1",
