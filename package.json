      "changeOrigin": false,
      "ws": false,
      "xfwd": true
    }
  },
  "eslintConfig": {
    "root": false,
    "env": {
      "node": true
    }
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "\\.test\\.[t|j]sx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    }
  }
