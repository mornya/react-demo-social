{
  "name": "react-demo-social",
  "version": "0.1.0",
  "description": "A demo React.js application for Social Share and Widget Libs (@mornya/react-social-libs).",
  "keywords": [
    "mornya",
    "react",
    "vessel",
    "demo",
    "social",
    "share",
    "widget"
  ],
  "homepage": "https://mornya.github.io/react-demo-social",
  "license": "MIT",
  "author": "mornya <mornya78@gmail.com>",
  "private": true,
  "repository": "https://github.com/mornya/react-demo-social.git",
  "bugs": "https://github.com/mornya/react-demo-social/issues",
  "scripts": {
    "login": "react-vessel login",
    "lint": "react-vessel lint",
    "clean": "react-vessel clean",
    "build": "react-vessel build",
    "start": "react-vessel start",
    "test": "react-vessel test",
    "analyze": "react-vessel analyze",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=6"
  },
  "dependencies": {
    "@mornya/react-helper": "latest",
    "@mornya/react-social-libs": "^2.0.6",
    "classnames": "latest",
    "immer": "latest",
    "normalize.css": "latest",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/jest-diff": "latest",
    "@types/node": "^12.6.2",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.1.1",
    "@types/react-router-dom": "^4.3.4",
    "gh-pages": "^2.0.1",
    "jest": "^24.8.0",
    "jest-transform-stub": "^2.0.0",
    "prop-types": "^15.7.2",
    "ts-jest": "latest",
    "tslint": "^5.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": {
    "/mock/": {
      "target": "http://localhost:8080",
      "pathRewrite": {
        "^/mock/": ""
      },
      "secure": false,
      "changeOrigin": false,
      "ws": false,
      "xfwd": true
    }
  },
  "eslintConfig": {
    "root": false,
    "env": {
      "node": true
    }
  },
  "jest": {
    "transform": {
      ".[t|j]sx?$": "ts-jest",
      ".(bmp|gif|jpe?g|png|svg|webp|s?css|eot|[t|o]tf|woff2?)$": "jest-transform-stub"
    },
    "testRegex": "\\.test\\.[t|j]sx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    }
  }
}
